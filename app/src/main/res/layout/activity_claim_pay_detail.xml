<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/space60"
    >

        <ImageView
            android:id="@+id/iv_return_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/space20"
            android:src="@mipmap/ic_return_back"
        />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="理赔"
            android:textColor="@color/black"
            android:textSize="@dimen/font16"
        />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/space1"
        android:background="@color/gray"
    />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/space15"
    >

        <TextView
            android:id="@+id/tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="id: %s"
        />

        <TextView
            android:id="@+id/tv_user_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="userId: %s"
        />

        <TextView
            android:id="@+id/tv_user_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="userName: %s"
        />

        <TextView
            android:id="@+id/tv_id_card_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="idCardNumber: %s"
        />

        <TextView
            android:id="@+id/tv_car_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="carNo: %s"
        />

        <TextView
            android:id="@+id/tv_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="amount: %s"
        />

        <TextView
            android:id="@+id/tv_accident_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="accidentTime: %s"
        />

        <TextView
            android:id="@+id/tv_accident_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="accidentLocation: %s"
        />

        <TextView
            android:id="@+id/tv_repair_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="repairLocation: %s"
        />

        <TextView
            android:id="@+id/tv_repair_days"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="repairDays: %s"
        />

        <TextView
            android:id="@+id/tv_del_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="delFlag: %s"
        />

        <TextView
            android:id="@+id/tv_create_by"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="createBy: %s"
        />

        <TextView
            android:id="@+id/tv_create_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="createDate: %s"
        />

        <TextView
            android:id="@+id/tv_update_by"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="updateBy: %s"
        />

        <TextView
            android:id="@+id/tv_update_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="updateDate: %s"
        />

    </LinearLayout>

</LinearLayout>
