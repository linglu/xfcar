<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/space44"
    >

        <ImageView
            android:id="@+id/iv_return_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/space20"
            android:src="@mipmap/ic_return_back"
        />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="车辆信息"
            android:textColor="@color/black"
            android:textSize="@dimen/font16"
        />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/space1"
        android:background="@color/gray"
    />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/space20"
    >

        <TextView
            android:id="@+id/tv_id"
            style="@style/common_text_view"
            android:text="id: "
        />

        <TextView
            android:id="@+id/tv_car_no"
            style="@style/common_text_view"
            android:text="carNo: "
        />

        <TextView
            android:id="@+id/tv_engine_no"
            style="@style/common_text_view"
            android:text="engineNo: "
        />

        <TextView
            android:id="@+id/tv_brand"
            style="@style/common_text_view"
            android:text="brand: "
        />

        <TextView
            android:id="@+id/tv_model_no"
            style="@style/common_text_view"
            android:text="modelNo: "
        />

        <TextView
            android:id="@+id/tv_status"
            style="@style/common_text_view"
            android:text="status: "
        />

        <TextView
            android:id="@+id/tv_type"
            style="@style/common_text_view"
            android:text="type: "
        />

        <TextView
            android:id="@+id/tv_username"
            style="@style/common_text_view"
            android:text="username: "
        />

        <TextView
            android:id="@+id/tv_user_id"
            style="@style/common_text_view"
            android:text="userId: "
        />

        <TextView
            android:id="@+id/tv_company"
            style="@style/common_text_view"
            android:text="company: "
        />

        <TextView
            android:id="@+id/tv_macid"
            style="@style/common_text_view"
            android:text="macid: "
        />

        <TextView
            android:id="@+id/tv_objectid"
            style="@style/common_text_view"
            android:textSize="@dimen/font_small"
            android:text="objectid: "
        />

        <TextView
            android:id="@+id/tv_del_flag"
            style="@style/common_text_view"
            android:text="delFlag: "
        />

        <TextView
            android:id="@+id/tv_create_by"
            style="@style/common_text_view"
            android:text="createBy: "
        />

        <TextView
            android:id="@+id/tv_create_date"
            style="@style/common_text_view"
            android:text="createDate: "
        />

        <TextView
            android:id="@+id/tv_update_by"
            style="@style/common_text_view"
            android:text="updateBy: "
        />

        <TextView
            android:id="@+id/tv_update_date"
            style="@style/common_text_view"
            android:text="updateDate: "
        />

    </LinearLayout>

</LinearLayout>
